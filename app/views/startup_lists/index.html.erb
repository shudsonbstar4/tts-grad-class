<div class="container">
  <h1>Listing startup_lists</h1>

  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Name</th>
        <th>Url</th>
        <th colspan="3">Actions</th>
      </tr>
    </thead>

    <tbody>
      <% @startup_lists.each do |startup_list| %>
        <tr>
          <td><%= startup_list.name %></td>
          <td><%= startup_list.url %></td>
          <td><%= link_to 'Show', startup_list %></td>
          <td><%= link_to 'Edit', edit_startup_list_path(startup_list) %></td>
          <td><%= link_to 'Destroy', startup_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <button class="btn btn-success"><%= link_to 'New Startup list', new_startup_list_path %></button>
</div>
